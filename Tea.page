    b4 ~ dnorm(0, .00001)
    b5 ~ dnorm(0, .00001)

    sigma ~ dunif(0, 100)
    tau <- pow(sigma, -2)

    tau.Temp.final  <- 1 / pow((2/14.093631),       2)

    tau.Volume.cups <- 1 / pow((0.05/0.6538625482), 2)
}
